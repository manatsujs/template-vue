{
  "name": "template-vue",
  "version": "0.2.1",
  "description": "Vue template for Manatsu",
  "type": "module",
  "packageManager": "pnpm@8.13.1",
  "private": true,
  "lint-staged": {
    "*.{?(c|m)@(j|t)s,css,vue,md,json}": "prettier --write",
    "*.{?(c|m)@(j|t)s,vue}": "pnpm lint"
  },
  "scripts": {
    "build": "run-p type-check build-only",
    "build-only": "vite build",
    "deps": "taze -f -w -l",
    "dev": "vite",
    "format": "prettier . --write",
    "format-check": "prettier . --check",
    "lint": "eslint . --config eslint.config.js --cache",
    "prepare": "husky install",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@vueuse/core": "^10.7.1",
    "manatsu": "^0.1.46",
    "vue": "^3.3.13"
  },
  "devDependencies": {
    "@tb-dev/eslint-config": "^1.5.4",
    "@types/node": "^20.10.5",
    "@vitejs/plugin-vue": "^5.0.0",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.1.1",
    "taze": "^0.13.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vue-tsc": "^1.8.27"
  }
}
