{
  "name": "template-vue",
  "version": "0.1.0",
  "description": "Vue template for Manatsu",
  "type": "module",
  "packageManager": "pnpm@8.10.5",
  "private": true,
  "lint-staged": {
    "*.{?(c|m)@(j|t)s,css,vue,md,json}": "prettier --write"
  },
  "scripts": {
    "build": "run-p type-check build-only",
    "build-only": "vite build",
    "dev": "vite",
    "prepare": "husky install",
    "prettier": "pnpm exec prettier . --write",
    "taze": "pnpm exec taze -w -f",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@vueuse/core": "^10.7.0",
    "manatsu": "^0.1.12",
    "vue": "^3.3.12",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@vitejs/plugin-vue": "^4.5.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.1.0",
    "taze": "^0.12.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2",
    "vite": "^4.5.0",
    "vite-plugin-vue-devtools": "1.0.0-rc.7",
    "vue-tsc": "^1.8.22"
  }
}
